  "version": "0.1.0",
  "homepage": "https://github.com/nexdynamic/npm-workspace",
  "author": {
    "name": "nexdynamic"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nexdynamic/bower-workspace"
  },
  "bugs": {
    "url": "https://github.com/nexdynamic/bower-workspace/issues"
  },
  "licenses": [
    {
      "type": "MIT",
